JaxDQNAgent.stack_size = %gym_lib.CARTPOLE_STACK_SIZE

JaxDQNAgent.gamma = 0.99
JaxDQNAgent.update_horizon = 1
JaxDQNAgent.min_replay_history = 500
JaxDQNAgent.update_period = 4
